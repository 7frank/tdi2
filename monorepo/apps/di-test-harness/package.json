{
  "name": "di-test-harness",
  "version": "1.0.0",
  "description": "Test harness for DI code generator using Ladle",
  "type": "module",
  "scripts": {
    "prepare:di": "node prepare-ladle.mjs",
    "dev": "npm run prepare:di && ladle serve",
    "dev:skip-di": "ladle serve",
    "build": "ladle build",
    "preview": "ladle preview",
    "clean": "rm -rf src/.tdi2 node_modules/.vite",
    "test:plugin": "node test-plugin.mjs"
  },
  "dependencies": {
    "ts-morph": "^21.0.1",
    "@tdi2/di-core": "workspace:*"
  },
  "devDependencies": {
    "@ladle/react": "^5.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "^5.2.2",
    "@tdi2/vite-plugin-di": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  }
}